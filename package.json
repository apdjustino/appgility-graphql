{
  "name": "appgility-graphql",
  "version": "0.0.1",
  "scripts": {
    "build": "tsc",
    "build:production": "npm run prestart && npm prune --production",
    "watch": "tsc --w",
    "prestart": "npm run build && func extensions install",
    "start:host": "func start",
    "start": "npm-run-all --parallel start:host watch",
    "test": "echo \"No tests yet...\"",
    "generate": "graphql-codegen --config codegen.yml",
    "format": "prettier-eslint \"graphql/**/*.ts\" \"graphql/**/*.tsx\" --write"
  },
  "description": "",
  "devDependencies": {
    "@azure/functions": "^1.0.1-beta1",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typescript": "1.22.3",
    "@graphql-codegen/typescript-resolvers": "1.19.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-html": "^6.1.2",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.0",
    "prettier-eslint-cli": "^5.0.1",
    "typescript": "^3.3.3"
  },
  "main": "index.js",
  "author": "Justin Martinez <justinmartinez14@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@azure/cosmos": "^3.11.5",
    "@types/azure": "^0.9.20",
    "apollo-server-azure-functions": "^2.25.2",
    "axios": "^0.21.4",
    "graphql": "^15.5.1",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.0.3",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  }
}
