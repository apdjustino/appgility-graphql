{
    "name": "appgility-graphql",
    "version": "0.0.1",
    "scripts": {
        "build": "tsc",
        "build:production": "npm run prestart && npm prune --production",
        "watch": "tsc --w",
        "prestart": "npm run build && func extensions install",
        "start:host": "func start",
        "start": "nodemon --exec 'ts-node' --logError ./graphql/server.ts",
        "test": "echo \"No tests yet...\"",
        "generate": "graphql-codegen --config codegen.yml",
        "format": "prettier-eslint \"graphql/**/*.ts\" \"graphql/**/*.tsx\" --write",
        "lint": "node_modules/eslint/bin/eslint.js --ext tsx,ts . --max-warnings=0"
    },
    "description": "",
    "devDependencies": {
        "@azure/functions": "1.0.1-beta1",
        "@babel/eslint-parser": "7.17.0",
        "@graphql-codegen/cli": "1.21.5",
        "@graphql-codegen/typescript": "1.22.3",
        "@graphql-codegen/typescript-resolvers": "2.4.3",
        "@types/node": "^20.14.8",
        "@typescript-eslint/eslint-plugin": "5.17.0",
        "@typescript-eslint/parser": "5.17.0",
        "eslint": "7.32.0",
        "eslint-config-prettier": "8.3.0",
        "eslint-plugin-html": "6.1.2",
        "eslint-plugin-prettier": "4.0.0",
        "nodemon": "3.1.4",
        "npm-run-all": "4.1.5",
        "prettier": "2.4.0",
        "prettier-eslint-cli": "5.0.1",
        "serverless-plugin-typescript": "2.1.5",
        "ts-node": "10.9.2",
        "typescript": "5.5.2"
    },
    "main": "index.js",
    "author": "Justin Martinez <justinmartinez14@gmail.com>",
    "license": "MIT",
    "dependencies": {
        "@apollo/datasource-rest": "6.3.0",
        "@apollo/server": "4.10.4",
        "@as-integrations/aws-lambda": "3.1.0",
        "@azure/cosmos": "3.11.5",
        "@graphql-tools/schema": "10.0.4",
        "@types/azure": "0.9.20",
        "@types/faker": "5.5.9",
        "axios": "0.21.4",
        "dotenv": "^16.4.5",
        "faker": "5.5.3",
        "graphql": "15.5.1",
        "graphql-tag": "2.12.6",
        "jsonwebtoken": "8.5.1",
        "jwks-rsa": "2.0.3",
        "lodash": "4.17.21",
        "uuid": "8.3.2",
        "yup": "1.4.0"
    }
}
